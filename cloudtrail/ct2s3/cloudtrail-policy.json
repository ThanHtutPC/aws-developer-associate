{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AWSCloudTrailAclCheck20150319-6c9d3e81-5cf3-4cfc-839e-f9a5e024d77d",
            "Effect": "Allow",
            "Principal": {
                "Service": "cloudtrail.amazonaws.com"
            },
            "Action": "s3:GetBucketAcl",
            "Resource": "arn:aws:s3:::cloudtraillogsins3",
            "Condition": {
                "StringEquals": {
                    "AWS:SourceArn": "arn:aws:cloudtrail:us-east-1:067120797866:trail/Cloudtrail2S3"
                }
            }
        },
        {
            "Sid": "AWSCloudTrailWrite20150319-d87c09ab-5729-4a62-bbc4-890cd57bcd8d",
            "Effect": "Allow",
            "Principal": {
                "Service": "cloudtrail.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::cloudtraillogsins3/main/AWSLogs/067120797866/*",
            "Condition": {
                "StringEquals": {
                    "AWS:SourceArn": "arn:aws:cloudtrail:us-east-1:067120797866:trail/Cloudtrail2S3",
                    "s3:x-amz-acl": "bucket-owner-full-control"
                }
            }
        }
    ]
}